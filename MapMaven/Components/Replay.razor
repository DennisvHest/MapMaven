@using MapMaven.Core.Services

<iframe src="@GetScoreSaberReplayUrl()" style="height: 80vh"></iframe>

@code {
    [Inject]
    protected IScoreSaberService ScoreSaberService { get; set; }

    [Parameter]
    public Map Map { get; set; }

    string GetScoreSaberReplayUrl()
    {
        return ScoreSaberService.GetScoreSaberReplayUrl(Map.Id, Map.HighestPlayerScore);
    }
}
